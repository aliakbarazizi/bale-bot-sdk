    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ali Akbar Azizi",
            "email": "aliakbar.azizi20@gmail.com"
        }
    ],
    "autoload": {
